<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToolbarTextStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
    </style>


    <style name="CustomCardView">
        <item name="android:background">@drawable/custom_card_view</item>
        <item name="android:elevation">6dp</item>
    </style>

    <style name="Form">
        <item name="android:layout_marginStart">12dp</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="Form.Section">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:gravity">start</item>
    </style>


    <style name="Form.Section1">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="Form.SectionBelitung">
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="Form.Label">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:gravity">start</item>
    </style>




    <style name="Form.Message">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.EditText">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.EditText_grey">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9C9C9C</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.MultilineEditText">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9C9C9C</item>
        <item name="android:background">@drawable/multiline_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.EditText_forgotpass">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9C9C9C</item>
        <item name="android:background">@color/gray</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.EditTextfilter">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.EditTextfilterAppy">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">@drawable/bg_edit_text_apply</item>
        <item name="android:minHeight">37dp</item>
        <item name="maxLines">1</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="Form.Spinner">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>

        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:drawableEnd">@drawable/ic_dropdown</item>
        <item name="android:drawablePadding">12dp</item>
        <item name="android:focusable">false</item>
        <item name="android:longClickable">false</item>
        <item name="android:cursorVisible">false</item>
    </style>

    <style name="Form.Spinner1">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9C9C9C</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:minHeight">37dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:gravity">start|center_vertical</item>

        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:drawableEnd">@drawable/ic_dropdown</item>
        <item name="android:drawablePadding">12dp</item>
        <item name="android:focusable">false</item>
        <item name="android:longClickable">false</item>
        <item name="android:cursorVisible">false</item>
    </style>

    <style name="Form.Button">
        <item name="android:minHeight">36dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_rounded_button_next</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:layout_marginTop">45dp</item>
        <item name="android:layout_marginBottom">38dp</item>
    </style>


    <style name="Form.Button_otp">
        <item name="android:minHeight">36dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_rounded_button_next</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="Form.Switch">
        <item name="trackTint">#E6E6E6</item>
        <item name="thumbTint">#9C9C9C</item>
    </style>

    <style name="NormalButtonStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bg_bottom</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="SmallButtonTicket">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#404040</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="TeamPicker">
        <item name="vpv_areDividersEnabled">true</item>
        <item name="vpv_isInfiniteScrollEnabled">false</item>
        <item name="vpv_maxVisibleItems">5</item>
        <item name="vpv_textColor">#404040</item>
        <item name="vpv_textSize">16sp</item>
        <item name="vpv_textTypeface">@font/roboto_bold</item>
        <item name="vpv_divider">@drawable/item_divider</item>
        <item name="vpv_orientation">vertical</item>
    </style>


    <style name="MyTablayoutItemStyle" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">true</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>


</resources>